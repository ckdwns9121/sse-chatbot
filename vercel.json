{
  "version": 2,
  "builds": [
    {
      "src": "apps/server/src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/server/src/main.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "apps/server/src/main.ts": {
      "maxDuration": 30
    }
  }
}
